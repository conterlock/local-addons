<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
    <record id="group_precio_A" model="res.groups">
      <field name="name">PrecioA</field>
      <field name="category_id" ref="base.module_category_hidden"/>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_mayorista" model="res.groups">
      <field name="name">Mayorista</field>
      <field name="category_id" ref="base.module_category_hidden"/>
      <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
  </data>

  <data noupdate="1">
    <record model="ir.rule" id="product_prices_rule_01">
      <field name="name">Precios de Productos Tarifa Precio A</field>
      <field name="model_id" ref="product.pricelist.item"/>
      <field name="groups" eval="[(4, ref('base.group_user'))]"/>
      <field name="domain_force">[('create_uid', '=', user.id)]</field>
    </record>
    <record model="ir.rule" id="product_prices_rule_01">
      <field name="name">Precios de Productos Tarifa Mayorista</field>
      <field name="model_id" ref="product.pricelist.item"/>
      <field name="groups" eval="[(4, ref('base.group_system'))]"/>
        <field name="domain_force">[(1, '=', 1)]</field>
      </record>
    </data>
</odoo>
